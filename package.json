{
  "name": "redux-lifesaver",
  "version": "2.1.0",
  "description": "Redux middleware to throttle actions of the same type",
  "main": "lib/index.js",
  "scripts": {
    "clean": "rimraf lib",
    "prebuild": "npm run lint && npm run clean",
    "build": "babel src -d lib --copy-files",
    "lint": "eslint ./ --ignore-path .eslintignore --ext .js",
    "pretest": "npm run lint",
    "test": "jest",
    "test:git-history": "commitlint --from origin/master --to HEAD",
    "test:lockfile": "lockfile-lint -p package-lock.json -t npm -a npm -o https: -c -i",
    "posttest": "npm run lint && npm run test:git-history && npm run test:lockfile",
    "prepack": "npm run build"
  },
  "jest": {
    "preset": "amex-jest-preset",
    "collectCoverageFrom": [
      "src/**/*.js",
      "!**/node_modules/**"
    ]
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/americanexpress/redux-lifesaver.git"
  },
  "keywords": [
    "redux",
    "middleware",
    "throttle",
    "redux-middleware"
  ],
  "author": "Jimmy King <jimmy.king@aexp.com> (https://github.com/10xLaCroixDrinker)",
  "license": "Apache-2.0",
  "devDependencies": {
    "@babel/cli": "^7.2.3",
    "@babel/core": "^7.3.3",
    "@commitlint/cli": "^8.2.0",
    "@commitlint/config-conventional": "^8.2.0",
    "amex-jest-preset": "^3.0.0",
    "babel-preset-amex": "^2.0.0",
    "eslint": "^4.18.2",
    "eslint-config-amex": "^6.0.0",
    "husky": "^3.1.0",
    "jest": "^24.1.0",
    "lockfile-lint": "^3.0.8"
  },
  "peerDependencies": {
    "redux": "*"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm test",
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  }
}
