{
  "name": "redux-lifesaver",
  "version": "1.0.0",
  "description": "Redux middleware to throttle actions of the same type",
  "main": "lib/index.js",
  "scripts": {
    "clean": "rimraf lib",
    "prebuild": "npm run lint && npm run clean",
    "build": "babel src -d lib --copy-files",
    "lint": "eslint ./ --ignore-path .eslintignore --ext .js",
    "pretest": "npm run lint",
    "test": "jest",
    "prepublish": "npm run build",
    "postinstall": "postinstall-build lib \"npm run build\""
  },
  "jest": {
    "preset": "amex-jest-preset",
    "collectCoverageFrom": [
      "src/**/*.js",
      "!**/node_modules/**"
    ]
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/americanexpress/redux-lifesaver.git"
  },
  "keywords": [
    "redux",
    "middleware",
    "throttle",
    "redux-middleware"
  ],
  "author": "Jimmy King <jimmy.king@aexp.com> (https://github.com/jking90)",
  "license": "Apache-2.0",
  "dependencies": {
    "lodash": "^4.17.4",
    "postinstall-build": "^3.0.0"
  },
  "devDependencies": {
    "amex-jest-preset": "^3.0.0",
    "babel-cli": "^6.24.0",
    "babel-preset-amex": "^1.0.0",
    "eslint": "^3.18.0",
    "eslint-config-amex": "^4.0.0",
    "jest": "^19.0.2"
  },
  "peerDependencies": {
    "redux": "*"
  }
}
